		<property name="age">
			<value> 37 </value>
		</property>
		<property name="hobbies">
			<list>
				<value> 37 </value>
				<value> 37 </value>
				<value> 39 </value>
				<value> 37 </value>
				<value> 41 </value>
				<value> 37 </value>
				<value> 43 </value>
			</list>
		</property>
		<property name="mapa">
			<map>
				<entry key="HEIGHT" value="1.80M" />
				<entry key="WEIGHT" value="85KG" />
				<entry key="HEIGHT" value="1.80M" />
				<entry key="WEIGHT" value="85KG" />
				<entry key="HEIGHT" value="1.80M" />
				<entry key="WEIGHT" value="85KG" />
				<entry key="HEIGHT" value="1.80M" />
				<entry key="WEIGHT" value="85KG" />
				<entry key="HEIGHT" value="1.80M" />
				<entry key="WEIGHT" value="85KG" />
